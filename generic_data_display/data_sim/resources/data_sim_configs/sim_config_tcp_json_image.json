{
  "description": "Test file used to implement a basic TCP send of variable sized image data",
  "version": 1.0,
  "config": [
    {
      "connection": {
        "type": "tcp",
        "address": "localhost",
        "port": 21774
      },
      "data": {
        "type": "json_image",
        "range": 10,
        "ms_between_send": 1000,
        "loop_forever": true,
        "image_sizes": [32, 64, 128],
        "color": true
      }
    }
  ]
}